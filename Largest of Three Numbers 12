#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
   int main() 
{
    int a,b,c;
    scanf("%d %d %d", &a,&b,&c);
    if(a-b>0 && a-c>0)
        {
            printf("a is largest then b and c");
        }
     else
       {
        if(b-c>0 && b-a)
            {
             printf("b is largest then a and c"); 
            }
         else 
            printf("c is largest then a and b");
        }
return 0;
}
