#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a=45,b=23,c=55,d=23,e=43;
    int sum;
    float avg;
    scanf("%d %d %d %d %d",&a,&b,&c,&d,&e);
    sum=a+b+c+d+e;
    avg=(sum)/5.0;
    printf("Sum of the 5 number is : %d\n",sum);
    printf("Average of the 5 number is : %.1f\n",avg);
    return 0;
    
}
