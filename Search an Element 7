#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     int n,i,value,count=0;
     scanf("%d",&n);
     int arr[n];
     for(i=0;i<n;i++)
      {
        scanf("%d",&arr[i]);
       }
    scanf("%d",&value);
    for(i=0;i<n;i++)
   {
        if(arr[i] == value){
            count=1;
            break;
      }
    } 
     if(count)
         printf("%d is presented in an array.\n",value);
    else
        printf("%d is not presented in an array.\n",value);
    return 0;
}
