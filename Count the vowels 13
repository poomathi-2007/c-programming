#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    char arr[100];
    int count=0;
    scanf("%s",arr);
    for(int i=0;arr[i]!='\0';i++)
    {
        char ch=arr[i];
      if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')
        {
            count++;
        }
    }
    printf("count: %d",count);
}
