#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int size;
    scanf("%d",&size);
    int arr[size];
    for(int i=0;i<size;i++)
        {
        scanf("%d",&arr[i]);
       }
    int min=arr[0];
     for(int i=0;i<size;i++)
     {
         if(min>arr[i]){
             min=arr[i];
         }
     }
    printf("Youngest=%d\n",min);
 int max=arr[0];
     for(int i=0;i<size;i++)
     {
         if(max<arr[i]){
             max=arr[i];
         }
     }
    printf("Oldest=%d",max);
}
